<link rel="stylesheet" type="text/css" href="/stylesheets/signup.css">
<div class="modal fade" id="englishModal" tabindex="-1" role="dialog" aria-labelledby="englishModal" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
          <div class="modal-content">
            <div class="modal-header text">
              <h5 class="modal-title" id="englishModal">Describe Knowledge of English:</h5>
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
            </div>

            <div class="modal-body">
                <form id="englishform" onsubmit="closeEnglishModal()">
                <textarea id="englishtext" rows="10" cols="50" required></textarea>
                </form>
            </div>

            <div class="modal-footer">
              <button type="submit" form="englishform" id="" class="btn btn-dark">Save changes</button>
            </div>
            
          </div>
        </div>
      </div>
